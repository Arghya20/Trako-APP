<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp"
        >
        <shape android:shape="rectangle" >
            <solid android:color="#C4FFFA" />
            <corners android:radius="50dp" />
        </shape>
    </item>


    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        >


        <shape android:shape="rectangle" >
            <gradient android:startColor="#A80808" android:endColor="#EEA088" android:angle="90" />
            <corners android:radius="50dp" />
        </shape>




    </item>

</layer-list>
